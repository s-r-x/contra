{
  "name": "contra.js",
  "version": "0.1.0",
  "author": "srx",
  "description": "A Swiss army knife for 2D vectors math",
  "main": "dist/contra.js",
  "scripts": {
    "start": "webpack --config webpack.config.js --watch --mode=development",
    "build": "webpack --config webpack.config.js --mode=production",
    "test": "ava -s",
    "test:watch": "ava --watch -s"
  },
  "ava": {
    "files": [
      "test/index.js"
    ],
    "sources": [
      "dist/contra.js"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/s-r-x/contra"
  },
  "keywords": [
    "vector",
    "2D"
  ],
  "license": "ISC",
  "module": "src/index.js",
  "homepage": "https://github.com/s-r-x/contra",
  "devDependencies": {
    "ava": "^1.2.1",
    "mocha": "^6.0.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
